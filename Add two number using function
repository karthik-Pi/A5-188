#include <stdio.h>

int addTwoNumbers(int a, int b)
{
    int result;
    result = a + b;
    return result;
}

int main()
{
    int num1, num2, sum;
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);
    sum = addTwoNumbers(num1, num2);
    printf("The sum is: %d", sum);
    return 0;
}
